{
    "site_name": "teedoc",
    "site_slogon": "happy to write",
    "site_root_url": "/",
    "site_domain": "teedoc.neucrack.com",
    "site_protocol": "https",
    "config_template_dir": "config",
    "source": "https://github.com/teedoc/teedoc.github.io/blob/main",
    "route": {
        "docs": {
            "/get_started/zh/": "docs/get_started/zh",
            "/develop/zh/": "docs/develop/zh",
            "/more/": "docs/more"
        },
        "pages": {
            "/": "pages/index/zh"
        },
        "assets": {
            "/static/": "static",
            "/get_started/assets/": "docs/get_started/assets"
        },
        "blog": {
            "/blog/": "blog"
        }
    },
    "translate": {
        "docs": {
            "/get_started/zh/": [ {
                    "url": "/get_started/en/",
                    "src": "docs/get_started/en"
                }
            ],
            "/develop/zh/": [ {
                    "url": "/develop/en/",
                    "src": "docs/develop/en"
                }
            ],
            "/more/": [ {
                "url": "/more/en/",
                "src": "docs/more/en"
            }
        ]
        },
        "pages": {
            "/": [ {
                    "url": "/en/",
                    "src": "pages/index/en"
                }
            ]
        }
    },
    "executable": {
        "python": "python3",
        "pip": "pip3"
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
                "mathjax": {
                    "enable": true
                }
            }
        },
        "teedoc-plugin-jupyter-notebook-parser":{
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-google-translate":{
            "from": "pypi",
            "config": {
                "doc_types": ["blog", "page", "doc"]
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "env":{
                    "main_color": "#4caf7d"
                }
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "Search"
            }
        },
        "teedoc-plugin-baidu-tongji": {
            "from": "pypi",
            "config": {
                "code": "4d52982572d5512e9762879ebf063c86"
            }
        },
        "teedoc-plugin-blog": {
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-ad-hint": {
            "from": "pypi",
            "config": {
                "type": "hint",
                "label": "â˜†",
                "content": "This is a hint example(support i18n)</br><a target=\"_blank\" href=\"https://github.com/teedoc/teedoc\">Give a â˜† star </a> if you like this project ðŸ¦€ðŸ¦€",
                "show_after_s": 432000,
                "date": "2021-11-16 14:40"
            }
        },
        "teedoc-plugin-comments-gitalk": {
            "from": "pypi",
            "config": {
                "env": {
                    "clientID": "9d215c8c4d2357fab5ba",
                    "clientSecret": "1d641e9f45f74a620a262091409b1d719b626755",
                    "repo": "teedoc.github.io",
                    "owner": "teedoc",
                    "admin": ["Neutree"],
                    "main_color": "#4caf7d",
                    "second_color": "#0a7d43",
                    "adminAutoCreate": false
                }
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css"
                ],
                "footer_items": [
                    "/static/js/custom.js"
                ],
                "env":{
                    "main_color": "#4caf7d",
                    "second_color": "#0a7d43"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots":{
        "User-agent": "*"
    },
    "layout_root_dir": "layout"
}
